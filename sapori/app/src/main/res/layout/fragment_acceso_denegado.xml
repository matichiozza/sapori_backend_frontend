<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/acceso_denegado_container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F0EBE4">

    <RelativeLayout
        android:id="@+id/r99zcxb3b0ff"
        android:layout_width="match_parent"
        android:layout_height="140dp"
        android:background="#F0EBE4"
        android:layout_marginBottom="20dp">

        <ImageView
            android:id="@+id/flecha"
            android:layout_width="38dp"
            android:layout_height="26dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="25dp"
            android:clickable="true"
            android:focusable="true"
            android:layout_marginTop="75dp"
            android:src="@drawable/flecha" />

        <ImageView
            android:id="@+id/logo_sapori"
            android:layout_width="90dp"
            android:layout_height="44dp"
            android:layout_alignParentTop="true"
            android:layout_marginTop="65dp"
            android:layout_centerInParent="true"
            android:src="@drawable/logo_sapori" />
    </RelativeLayout>

    <ImageView
        android:layout_width="61dp"
        android:layout_height="77dp"
        android:layout_gravity="center"
        android:layout_marginBottom="106dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="65dp"
        android:layout_centerInParent="true"
        android:src="@drawable/candado" />

    <TextView
        android:id="@+id/tv_acceso_denegado"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ACCESO DENEGADO\n¡Para acceder debes iniciar sesión!"
        android:textSize="13sp"
        android:fontFamily="@font/poppinsbold"
        android:textColor="#4D4D4D"
        android:gravity="center"
        android:layout_marginTop="30dp"
        android:layout_gravity="center"
        android:padding="32dp" />
</FrameLayout>
